{
    "version": 2,
    "builds": [
      {
        "src": "dist/**/*.html",
        "use": "@vercel/static-build"
      },
      {
        "src": "dist/assets/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/dist/assets/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }
  